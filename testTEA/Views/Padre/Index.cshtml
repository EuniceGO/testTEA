@{
    Layout = "~/Views/Shared/_LayoutPadre.cshtml";
    ViewBag.Title = "Dashboard";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Dashboard</title>
    <link href="~/css/Preguntas.css" rel="stylesheet" />
    
</head>
<body>
    <div class="container">
        <div class="dashboard-content">
            <h2>Bienvenido Padre de Familia</h2>
            <form asp-action="CreateCuestionario" method="post">
                <button type="submit" class="btn btn-primary">Crear Cuestionario</button>
            </form>
            <a asp-action="IndexPreguntas" asp-controller="Preguntas">Ver preguntas</a>
        </div>
    </div>
</body>
</html>
